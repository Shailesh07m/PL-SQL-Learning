CREATE OR REPLACE PROCEDURE PP IS
    ID   EMP10.E_ID%TYPE;
    NAME EMP10.E_NAME%TYPE;
    CITY EMP10.E_CITY%TYPE;

    CURSOR C2 IS
        SELECT E_ID, E_NAME, E_CITY
        FROM EMP10
        WHERE SAL > 10000;
BEGIN
    OPEN C2;
    LOOP 
        FETCH C2 INTO ID, NAME, CITY;
        EXIT WHEN C2%NOTFOUND;

        DBMS_OUTPUT.PUT_LINE(ID || '     ' || NAME || '       ' || CITY);
    END LOOP;
    CLOSE C2;
END PP;
/


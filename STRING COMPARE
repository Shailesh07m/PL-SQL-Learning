CREATE OR REPLACE FUNCTION STR (ST1 IN VARCHAR2  , ST2 VARCHAR2)
RETURN VARCHAR2
AS
	L1 NUMBER;
	L2 NUMBER;
BEGIN
	L1:=length(ST1);
	L2:=length(ST2);
	IF ( L1> L2) THEN 
	RETURN ' FIRST STRING IS LARGER';
	ELSIF (L2 > L1) THEN
	RETURN' SECOND STRING IS LARGER';
	ELSE
	RETURN ' BOTH ARE EQUAL';
END IF;
END STR;
/

DECLARE
   BEGIN
   DBMS_OUTPUT.PUT_LINE(STR('HELLO','WORLD'));
   END;
   /
